    .word 76, -1, 76, -1, 74, -1, 74, -1, 74, 74, 74, 74, 74, 74, 74, 74
index: .word 0
num_pitches:  .word 4608  # Update this if needed

.text
.globl main


main:
    la $t0, BGM           # Load address of BGM array
    lw $t1, num_pitches   # Load total number of pitches
    li $t2, 0             # Counter (loop index)

loop:
    bge $t2, $t1, end     # If index >= total pitches, exit loop

    lw $a0, 0($t0)        # Load current pitch
    #beq $a0, -1, skip     # If pitch is -1, skip playing

    li $v0, 31            # Syscall to play sound
    li $a1, 200           # Duration
    li $a2, 11            # Instrument
    li $a3, 127           # Volume
    syscall

    li $v0, 32            # Syscall for sleep
    li $a0, 200           # Delay (adjust as needed)
    syscall

skip:
    addi $t0, $t0, 4      # Move to next pitch
    addi $t2, $t2, 1      # Increment index
    j loop                # Repeat loop

end:
    li $v0, 10            # Exit program
    syscall
